
#tutorial .dialog .step .step_image
  background-color: transparent
  background-image: url("/images/sprites/css_sprites.png")
  background-attachment: scroll
  background-repeat: no-repeat
  width: 40px
  height: 9px
  margin: 40px auto

#tutorial .dialog .stepIntro .step_image
  background-position: -143px -329px

#tutorial .dialog .stepDescribe .step_image
  background-position: -143px -339px

#tutorial .dialog .stepAnnotate .step_image
  background-position: -143px -349px

#tutorial .dialog .stepShowTransits .step_image
  background-position: -143px -359px
  margin: 30px aut
  
#tutorial .dialog .stepAnnotate .actions
  margin: 0px auto
  width: 260px

#favorite_and_download a
  color: white
  text-decoration: none

#favorite_and_download .separator
  padding-left: 1px
  background-color: #999999
  margin: 0 10px
  font-size: 10px

#favorite_and_download .fav,
#favorite_and_download .download
  display: inline-block

#favorite_and_download .fav
  background: url("/images/sprites/css_sprites.png") no-repeat scroll 0 -327px transparent
  height: 12px
  margin: 0 5px -1px 0
  width: 14px

#favorite_and_download .download
  background: url("/images/sprites/css_sprites.png") no-repeat scroll -14px -329px transparent
  height: 9px
  margin: 0 6px 0 1px
  vertical-align: middle
  width: 9px

#transits
  height: 440px
  left: 50px
  position: absolute
  overflow: hidden
  top: 5px
  width: 580px
  z-index: 10

#transits .transit
  border: 3px solid #FFCC00
  background-color: rgba(255,255,255,0.1)
  position: absolute
  z-index: 15

#transits .transit.focus
  border-color: #0099CC

#transits .transit .number
  background: url("/images/sprites/css_sprites.png") no-repeat scroll -312px -266px transparent
  display: none
  height: 20px
  left: -11px
  margin-top: -10px
  position: absolute
  text-align: center
  top: 50%
  width: 20px
  color: black
  line-height: 20px
  font-family: Tahoma
  font-size: 12px
  font-weight: bold

#transits .transit.focus .number
  background: url("/images/sprites/css_sprites.png") no-repeat scroll -332px -266px transparent

#transits .transit .resize_width,
#transits .transit .resize_height
  background: url("/images/sprites/css_sprites.png") no-repeat scroll -312px -266px transparent
  display: none
  position: absolute
  width: 20px
  height: 20px

#transits .transit .resize_width
  background-position: -372px -266px
  margin-top: -10px
  right: -12px
  top: 50%

#transits .transit .resize_height
  background-position: -352px -266px
  bottom: -11px
  left: 50%
  margin-left: -10px

#transits .transit.focus .resize_width,
#transits .transit.focus .resize_height
  display: block

.mag_glass
  background: url("/images/sprites/css_sprites.png") no-repeat scroll -705px -1px transparent
  color: #333333
  display: none
  font-size: 15px
  height: 170px
  padding: 13px 10px
  position: absolute
  text-shadow: 0 1px 0 #EECC60
  width: 152px
  z-index: 20

#mag_glass
  top: 0px
  left: 0px

.spinner
  margin-left: 390px
  margin-top: 215px
  z-index: 100
  position: relative


